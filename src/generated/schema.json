    },
    "response": {
      "type": "object",
      "properties": {
        "access_token": {"type": "string"},
        "user": {
          "type": "object",
          "properties": {
            "username": {"type": "string"},
            "discriminator": {"type": "string"},
            "id": {"type": "string"},
